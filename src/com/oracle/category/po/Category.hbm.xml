<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oracle.category.po">
	<class name="Category" table="T_Category">
		<id  name="categ_Id">
			<generator class="native"/>
		</id>
		<property name="categName" not-null="true" length="100"/>
		<property name="categContent" not-null="true" length="100"/>
		<property name="creator" not-null="true" length="100"/>
		<property name="createDate" not-null="true" length="100"/>
		<property name="updater" not-null="true" length="100"/>
		<property name="updateDate" not-null="true" length="100"/>
		
	</class>
</hibernate-mapping>