<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.oracle.quesType.po">
	<class name="QuesType" table="T_QuesType">
		<id  name="quesType_Id">
			<generator class="native"/>
		</id>
		<property name="quesTypeName" not-null="true" length="100"/>
		<property name="crator" not-null="true" length="100"/>
		<property name="createDate" not-null="true" length="100"/>
		<property name="updater" not-null="true" length="100"/>
		<property name="updateDate" not-null="true" length="100"/>
		
	</class>
</hibernate-mapping>